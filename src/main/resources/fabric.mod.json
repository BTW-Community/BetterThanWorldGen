{
  "schemaVersion": 1,
  "id": "btwg",
  "version": "${version}",
  "name": "Better than World Gen",
  "description": "A world generation overhaul mod for Better than Wolves: CE",
  "authors": [
    "The Lady Dawn / Dawnraider00"
  ],
  "contact": {
    "homepage": "https://github.com/BTW-Community/BetterThanWorldGen",
    "sources": "https://github.com/BTW-Community/BetterThanWorldGen"
  },
  "license": "MPL v2.0",
  "icon": "assets/btwg/icon.png",
  "environment": "*",
  "entrypoints": {
    "btw:addon": [
      "btwg.mod.BetterThanWorldGen"
    ],
    "emi": [
      "btwg.mod.crafting.BTWGEMIPlugin"
    ]
  },
  "custom": {
    "btw:addon_prefix": "BTWG"
  },
  "mixins": [
    "btwg.mixins.json"
  ],
  "accessWidener" : "btwg.accesswidener",
  "depends": {
    "fabricloader": ">=0.14.19",
    "minecraft": "1.6.4"
